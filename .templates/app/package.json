    "chart.js": "^2.7.2",
    "lodash": "^4.17.10",
    "luxon": "^1.3.3",
    "numeral": "^2.0.6",
    "nuxt": "^1.4.2",
    "vue-chartjs": "^3.4.0",
    "vue-markdown": "^2.2.4",
    "vue-mousetrap": "^1.0.1",
    "vuetify": "^1.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.5.0",
