  "description": "VuePoint.js Solution App - __app-key__",
  "author": "__author-name__",
  "license": "MIT",
  "private": true,
  "config": {
    "vp-solution-role": "app",
    "vp-suite-key": "__suite-key__",
    "vp-app-key": "__app-key__"
  },
  "scripts": {
    "dev": "nuxt --port 33770",
    "dev:explicit:port": "nuxt --port 33770",
    "dev-debug": "node --inspect node_modules/nuxt/bin/nuxt",
